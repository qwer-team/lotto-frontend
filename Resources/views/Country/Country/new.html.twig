{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Country creation</h1>

    <form action="{{ path('country_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
           <button type="submit" class="btn btn-primary" data-loading-text="Loading...">{% trans%}Create{%endtrans%}</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('country') }}">
            {% trans%}Back to the list{%endtrans%}
        </a>
    </li>
</ul>
{% endblock %}

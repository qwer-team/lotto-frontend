<!DOCTYPE html>
<html>
<head>
    {% block title %}
	<meta charset="UTF-8">
	<title>Document</title>
    {% endblock %}
    {% block css %}
        <link rel="stylesheet" href="{{ asset( "bundles/qwerlottofrontend/css/jquery-ui-1.10.2.custom.css") }}" media="all">
        <link rel="stylesheet" href="{{ asset( "bundles/qwerlottofrontend/css/imageflow.packed.css") }}">
        <link rel="stylesheet" href="{{ asset( "bundles/qwerlottofrontend/css/jquery.selectbox.css") }}" media="all">
        <link rel="stylesheet" href="{{ asset( "bundles/qwerlottofrontend/css/style.css") }}" media="all">
    {% endblock %}
    {% block javascript %}
	<script src="{{ asset( "bundles/qwerlottofrontend/js/jquery-1.9.1.js") }}"></script>
	<script src="{{ asset( "bundles/qwerlottofrontend/js/imageflow.packed.js") }}"></script>
        <script src="{{ asset( "bundles/qwerlottofrontend/js/imageflow.js") }}"></script>
	<script src="{{ asset( "bundles/qwerlottofrontend/js/jquery-ui-1.10.2.custom.min.js") }}"></script>
        <script src="{{ asset( "bundles/qwerlottofrontend/js/jquery.ui.datepicker.js") }}"></script>
	<script src="{{ asset( "bundles/qwerlottofrontend/js/jquery.ui.datepicker-"~locale~".js") }}"></script>	
	<script src="{{ asset( "bundles/qwerlottofrontend/js/jquery-ui-timepicker-addon.js") }}"></script>
        <script src="{{ asset( "bundles/qwerlottofrontend/js/jquery.selectbox-0.2.js") }}"></script>
        <script src="{{ asset( "bundles/qwerlottofrontend/js/script.js") }}"></script>
        <script>
                $(document).ready(function(){
                    
	/* new */
	$('.start_dater, .finish_dater').datetimepicker({
		timeOnlyTitle: '{{ 'date_picker.time_only_title'|trans}}',
		timeText: '{{ 'date_picker.time_text'|trans}}',
		hourText: '{{ 'date_picker.hour_text'|trans}}',
		minuteText: '{{ 'date_picker.minute_text'|trans}}',
		secondText: '{{ 'date_picker.second_text'|trans}}',
		currentText: '{{ 'date_picker.current_text'|trans}}',
		closeText: '{{ 'date_picker.close_text'|trans}}',
		dateFormat: 'yy-mm-dd',
		timeFormat: 'HH:mm'
                
	});
                });
                var expand = '{{ "index.expand"|trans }}';
                var minimize = '{{ "index.minimize"|trans }}';
                var locale = '{{ locale }}';
        </script>
     {% endblock %}
</head>

<body>
    
	<!--<div id="header">
		<img src="{{ asset( "bundles/qwerlottofrontend/images/header.png") }}" alt="">
	</div>-->
	<div id="container">
		<div class="main-container"><!-- main-container -->
                    
                        {{ render(controller('QwerLottoFrontendBundle:FrontEndPage:leftMenu', {'_locale' : locale, 'menu': menu})) }}
                    
			<div class="main-col"><!-- main-col -->
                            {% block content %}
                            {% endblock %}
				<div class="bottom"></div>
			</div><!-- .main-col -->
		</div><!-- .main-container -->
	</div>
</body>
</html>